{
    "name": "aamant/magento2-docker",
    "description": "Docker development stack for Magento",
    "type": "magento2-component",
    "minimum-stability": "stable",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Amant Arnaud",
            "email": "contact@arnaudamant.fr"
        }
    ],
    "require": {
        "php": ">=7.3",
        "aamant/composer-copy": "*"
    },
    "extra": {
        "map": {
            "aamant/magento2-docker": [
                "docker-compose.proxy.yml.dist",
                "docker-compose.yml.dist"
            ]
        }
    },
    "repositories": [
        {
            "url": "/Users/arnaud/workspace/repositories/composer-copy",
            "type": "path",
            "options": {
                "symlink": true
            }
        }
    ]
}
